<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:text="Riwayat Kelas"
        android:textSize="25dp"/>
        <LinearLayout
            android:id="@+id/linear_layout_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/button_margin"
            android:layout_marginLeft="30dp"
            android:layout_below="@+id/linear_layout_3">

            <ImageView
                android:id="@+id/iv_logo6"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:src="@drawable/newlogok"/>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:orientation="vertical"
                android:layout_marginLeft="50dp">

                <TextView
                    android:id="@+id/tv_detail8"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Ruangan LAB CA.KOM 1,3 "
                    android:textSize="15dp"
                    android:textColor="@color/black"
                    android:ems="10"/>

                <TextView
                    android:id="@+id/tv_matkul8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:gravity=""
                    android:text="Sistem Klien Server" />

                <TextView
                    android:id="@+id/tv_nama8"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:text="07.00 - 11.00"
                    android:textSize="12dp"/>

            </LinearLayout>
        </LinearLayout>
    <LinearLayout
        android:id="@+id/linear_layout_8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="30dp"
        android:layout_below="@+id/linear_layout_6">

        <ImageView
            android:id="@+id/iv_logo8"
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:src="@drawable/newlogok"/>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_marginLeft="50dp">

            <TextView
                android:id="@+id/tv_detail7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ruangan LAB CA.KOM 2,4 "
                android:textSize="15dp"
                android:textColor="@color/black"
                android:ems="10"/>

            <TextView
                android:id="@+id/tv_matkul7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity=""
                android:text="Pemrosesan Data Elektronik" />

            <TextView
                android:id="@+id/tv_nama7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:text="07.00 - 11.00"
                android:textSize="12dp"/>

        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/linear_layout_9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="18dp"
        android:layout_marginLeft="30dp"
        android:layout_below="@+id/linear_layout_8">

        <ImageView
            android:id="@+id/iv_logo9"
            android:layout_width="80dp"
            android:layout_height="50dp"
            android:src="@drawable/newlogok"/>


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="vertical"
            android:layout_marginLeft="50dp">

            <TextView
                android:id="@+id/tv_detail9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ruangan LAB CB.KOM 1,3 "
                android:textSize="15dp"
                android:textColor="@color/black"
                android:ems="10"/>

            <TextView
                android:id="@+id/tv_matkul9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:gravity=""
                android:text="Pemrograman Web" />

            <TextView
                android:id="@+id/tv_nama9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ems="10"
                android:text="07.00 - 11.00"
                android:textSize="12dp"/>

        </LinearLayout>
    </LinearLayout>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/linear_layout_9"
        android:layout_marginTop="24dp"
        android:layout_marginLeft="10dp"
        android:text="Riwayat Kendaraan"
        android:textSize="25dp"/>


</RelativeLayout>